         function HertzzController($scope,$http) {
             $http.get("usuario", {params:{usuario: window.location.search.replace("?","")}}).success( function(response) { //El window.location.search.replace("?","") solo retorna lo que hay despues del ? en la URL actual
            	$scope.usuario = response; 
             });
      
         }
    </script>
